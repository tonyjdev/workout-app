{
  "name": "workout-app",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "android": "npm run build && npx cap sync android && npx cap open android",
    "sync:android-version": "node scripts/sync-android-version.mjs",
    "release:patch": "npm version patch && npm run sync:android-version && npx capacitor sync android",
    "release:minor": "npm version minor && npm run sync:android-version && npx capacitor sync android",
    "release:major": "npm version major && npm run sync:android-version && npx capacitor sync android"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor/android": "7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "7.0.2",
    "@capacitor/ios": "7.4.4",
    "@capacitor/keyboard": "7.0.3",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "pinia": "^3.0.3",
    "vue": "^3.3.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@eslint/js": "^9.38.0",
    "@types/bootstrap": "^5.2.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^13.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.6.2",
    "terser": "^5.4.0",
    "typescript": "~5.6.2",
    "vite": "7.1.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.1.10"
  },
  "overrides": {
    "rollup": "4.44.0"
  },
  "description": "An Ionic project"
}
